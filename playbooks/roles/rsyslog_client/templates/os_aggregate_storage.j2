{% if log_files is defined and log_files.stdout_lines is defined %}
{% set rsyslog_client_log_files = log_files.stdout_lines | union(rsyslog_client_log_files) %}
{% endif %}
{% for log_file in rsyslog_client_log_files %}
{{ log_file }}
{% endfor %}
{
        copytruncate
        weekly
        missingok
        rotate 14
        compress
        dateext
        maxage 60
        notifempty
        nocreate
}
